{
  "name": "bitbucket-connector",
  "version": "1.0.0",
  "description": "verhy coo prozhect, i liketh ith",
  "main": "index.js",
  "scripts": {
    "start": "npm run build & node dist/index.js",
    "build": "npm run clean & webpack --progress",
    "dev": "npm run clean & cross-env NODE_ENV=development webpack --progress",
    "clean": "rimraf dist",
    "init:all": "npm run init:hooks & npm run init:node_modules",
    "init:node_modules": "rimraf node_modules & unzip node_modules",
    "init:hooks": "ncp ./hooks ./.git/hooks"
  },
  "author": "Hilda",
  "license": "ISC",
  "dependencies": {
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "express": "4.17.1",
    "express-async-handler": "1.1.4",
    "jsonwebtoken": "9.0.2",
    "request": "2.88.2",
    "typescript": "5.6.3"
  },
  "devDependencies": {
    "@types/cors": "2.8.9",
    "@types/express": "5.0.0",
    "@types/jsonwebtoken": "9.0.7",
    "@types/node": "20.17.0",
    "builtin-modules": "4.0.0",
    "cross-env": "7.0.3",
    "husky": "9.1.6",
    "lint-staged": "15.2.10",
    "ncp": "2.0.0",
    "nodemon": "3.1.7",
    "nodemon-webpack-plugin": "4.3.2",
    "prettier": "3.3.3",
    "rimraf": "6.0.1",
    "ts-loader": "9.5.1",
    "webpack": "5.11.0",
    "webpack-cli": "5.1.4",
    "webpack-node-externals": "3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,css,md}": "prettier --write"
  }
}
