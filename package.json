{
  "name": "bitbucket-connector",
  "version": "1.0.0",
  "description": "verhy coo prozhect, i liketh ith",
  "main": "index.js",
  "scripts": {
    "start": "npm run build & node dist/index.js",
    "build": "npm run clean & webpack --progress",
    "dev": "npm run clean & cross-env NODE_ENV=development webpack --progress",
    "clean": "rimraf dist",
    "init:all": "npm run init:hooks & npm run init:node_modules",
    "init:node_modules": "rimraf node_modules & unzip node_modules",
    "init:hooks": "ncp ./hooks ./.git/hooks"
  },
  "author": "Hilda",
  "license": "ISC",
  "dependencies": {
    "cookie-parser": "1.4.7",
    "cors": "2.8.5",
    "express": "4.21.1",
    "express-async-handler": "1.2.0",
    "jsonwebtoken": "8.5.1",
    "request": "2.88.2",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/jsonwebtoken": "8.5.9",
    "@types/node": "14.18.63",
    "builtin-modules": "3.3.0",
    "cross-env": "7.0.3",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "ncp": "2.0.0",
    "nodemon": "2.0.22",
    "nodemon-webpack-plugin": "4.8.2",
    "prettier": "2.8.8",
    "rimraf": "3.0.2",
    "ts-loader": "8.4.0",
    "webpack": "5.95.0",
    "webpack-cli": "4.10.0",
    "webpack-node-externals": "2.5.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,css,md}": "prettier --write"
  }
}
