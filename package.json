{
  "name": "bitbucket-connector",
  "version": "1.0.0",
  "description": "verhy coo prozhect, i liketh ith",
  "main": "index.js",
  "scripts": {
    "start": "npm run build & node dist/index.js",
    "build": "npm run clean & webpack --progress",
    "dev": "npm run clean & cross-env NODE_ENV=development webpack --progress",
    "clean": "rimraf dist",
    "init:all": "npm run init:hooks & npm run init:node_modules",
    "init:node_modules": "rimraf node_modules & unzip node_modules",
    "init:hooks": "ncp ./hooks ./.git/hooks"
  },
  "author": "Hilda",
  "license": "ISC",
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "request": "^2.88.2",
    "typescript": "^4.0.0"
  },
  "devDependencies": {
    "@types/cors": "2.8.8",
    "@types/express": "4.17.9",
    "@types/jsonwebtoken": "8.5.0",
    "@types/node": "14.14.8",
    "builtin-modules": "3.1.0",
    "cross-env": "7.0.2",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "nodemon": "2.0.6",
    "prettier": "2.1.2",
    "ts-loader": "8.0.9",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12",
    "webpack-node-externals": "2.5.2",
    "webpack-shell-plugin": "0.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,md}": "prettier --write"
  }
}
